model AssociationClassModel

class Company
attributes
  name : String 
  town : String
end

class Person
attributes
  ssn : String  
  age : Integer
end

associationclass WorksFor
between
  Company[0..1] role employer
  Person[1..*] role employee
attributes
  salary : Integer
end

associationclass EmployeeRanking
between
  Person[*] role bosses
  Person[*] role employees
attributes
  score : String
end

