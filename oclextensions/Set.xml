<extensions>
	<operation source="Set(Integer)" name="avg" returnType="Real">
		<body><![CDATA[
			sum = 0
			$self.each {|i|
				sum += i
			}
			
			return (sum.to_f / $self.length.to_f)
		]]>
		</body>
	</operation>
	<operation source="Collection(OclAny)" name="single" returnType="OclAny">
		<body><![CDATA[
			if $self.size == 1
			  $self.first
			else
			  nil
			end
		]]>
		</body>
	</operation>
</extensions>